ldapsearch -x -H "ldap://192.168.200.5" -D '' -w '' -b "DC=mike,DC=domain" | grep sAMAccountName:
ldapsearch -v -x -b "DC=mike,DC=com" -H "ldap://192.168.200.5" "(objectclass=*)" | grep -i -e 'pass\|name\|desc' 
